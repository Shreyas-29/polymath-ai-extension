{
    "manifest_version": 3,
    "name": "Polymath AI URL Saver",
    "version": "1.1",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "/icons/icon-16x16.png",
        "48": "/icons/icon-48x48.png",
        "512": "/icons/icon-512x512.png"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}